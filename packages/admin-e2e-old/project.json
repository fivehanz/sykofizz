{
  "name": "admin-e2e-old",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/admin-e2e-old/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "packages/admin-e2e-old/cypress.config.ts",
        "devServerTarget": "admin-old:serve:development",
        "testingType": "e2e"
      },
      "configurations": {
        "production": {
          "devServerTarget": "admin-old:serve:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/admin-e2e-old/**/*.{js,ts}"]
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["admin-old"]
}
